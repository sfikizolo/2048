    "version": "0.2.0",
    "configurations": [
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "type": "extensionHost"
        },
      
        {
            "name": "Python:Pytho3 2048.py",
            "type": "python",
            "request": "launch",
            "program": "${2048.py}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}